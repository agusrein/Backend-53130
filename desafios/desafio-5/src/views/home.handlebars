
<div class="d-flex flex-wrap col-12">
{{#each products}}

<div class="card col-4 m-3" style="width: 18rem;">
  <img src="..." class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
    <p class="card-text">{{this.description}}</p>
    <p>$ {{this.price}}</p>
  </div>
</div>
{{/each}}
</div>

